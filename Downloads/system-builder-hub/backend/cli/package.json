    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "yaml": "^2.3.4",
    "archiver": "^6.0.1",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
