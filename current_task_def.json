            "essential": true,
            "environment": [
                {
                    "name": "AWS_REGION",
                    "value": "us-west-2"
                },
                {
                    "name": "DATABASE_URL",
                    "value": "postgresql://sbhadmin:BrokenWood2025!@sbh-db-dev.cbksw4k0sknh.us-west-2.rds.amazonaws.com:5432/sbh_db"
                },
                {
                    "name": "AWS_BUCKET_NAME",
                    "value": "sbh-workspace-dev-776567512687"
                },
                {
                    "name": "S3_BUCKET_NAME",
                    "value": "sbh-workspace-dev-b8aedb34"
                },
                {
                    "name": "FLASK_ENV",
                    "value": "production"
                },
                {
                    "name": "SECRET_KEY",
                    "value": "super-secret-key"
                },
                {
                    "name": "OPENAI_API_KEY",
                    "value": "sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
                },
                {
                    "name": "ENV",
                    "value": "production"
                },
                {
                    "name": "CORS_ORIGINS",
